#%Module1.0#####################################################################
##
## modules modulefile
##
## modulefiles/modules.  Generated from modules.in by configure.
##
proc ModulesHelp { } {
        global version modroot

        puts stdout "\t loads intel 13.1.3 \n"
}

module-whatis   "loads intel 13.1.3"

prepend-path    PATH                    /opt/intel/composer_xe_2013/bin/intel64

prepend-path    LD_LIBRARY_PATH         /opt/intel/composer_xe_2013/debugger/lib/intel64
#prepend-path    INCLUDE                 /opt/intel/12.1.5/include 
#prepend-path    CPATH                   /opt/intel/12.1.5/include 
prepend-path    LIBRARY_PATH            /opt/intel/composer_xe_2013/debugger/lib/intel64
prepend-path    NLS_PATH                /opt/intel/composer_xe_2013/debugger/intel64/locale/%l_%t/%N

if [ module-info mode load ] {
 system echo "intel 13.1.3 loaded"
}

if [ module-info mode switch2 ] {
 system echo "intel 13.1.3 loaded"
}

if [ module-info mode remove ] {
 system echo "intel 13.1.3 unloaded"
}

